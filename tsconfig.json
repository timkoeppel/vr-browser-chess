{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noResolve": false,
    "noImplicitAny": false,
    "noEmit": false,
    "sourceMap": true,
    "preserveConstEnums":true,
    "lib": [
      "dom",
      "es2020"
    ],
    "types": [
      "babylonjs",
      "babylonjs-gui"
    ],
    "rootDir": "src",
    "outDir": "dist",
    "declaration": false,
    "moduleResolution": "Node",
    "emitDeclarationOnly": false,
    "listEmittedFiles": true,
    "watch": true
  }
}